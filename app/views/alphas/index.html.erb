<p id="notice"><%= notice %></p>

<h1>Listing Alphas</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Degree</th>
      <th>College</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alphas.each do |alpha| %>
      <tr>
        <td><%= alpha.name %></td>
        <td><%= alpha.email %></td>
        <td><%= alpha.degree %></td>
        <td><%= alpha.college %></td>
        <td><%= link_to 'Show', alpha %></td>
        <td><%= link_to 'Edit', edit_alpha_path(alpha) %></td>
        <td><%= link_to 'Destroy', alpha, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Alpha', new_alpha_path %>
